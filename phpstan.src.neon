        - '#Method League\\Period\\ProxyInterval::endingOn\(\) has parameter \$datepoint with no typehint specified.#'
        - '#Method League\\Period\\ProxyInterval::equalsTo\(\) has parameter \$interval with no typehint specified.#'
        - '#Method League\\Period\\ProxyInterval::expand\(\) has parameter \$duration with no typehint specified.#'
        - '#Method League\\Period\\ProxyInterval::gap\(\) has parameter \$interval with no typehint specified.#'
        - '#Method League\\Period\\ProxyInterval::getDatePeriod\(\) has parameter \$duration with no typehint specified.#'
        - '#Method League\\Period\\ProxyInterval::intersect\(\) has parameter \$interval with no typehint specified.#'
        - '#Method League\\Period\\ProxyInterval::move\(\) has parameter \$duration with no typehint specified.#'
        - '#Method League\\Period\\ProxyInterval::overlaps\(\) has parameter \$interval with no typehint specified.#'
        - '#Method League\\Period\\ProxyInterval::split\(\) has parameter \$duration with no typehint specified.#'
        - '#Method League\\Period\\ProxyInterval::splitBackwards\(\) has parameter \$duration with no typehint specified.#'
        - '#Method League\\Period\\ProxyInterval::startingOn\(\) has parameter \$datepoint with no typehint specified.#'
